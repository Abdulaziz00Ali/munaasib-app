# This file contains environment variables for the application.
# Copy this file to .env and fill in the values.

# Set to "true" to enable demo mode. In demo mode, certain features like
# real authentication and submissions are disabled.
VITE_DEMO_MODE=true

# The WhatsApp number used for contacting support or making bookings.
# Use international format without any symbols or spaces (required by wa.me).
# Example (Saudi): 9665XXXXXXXX
# مثال (السعودية): 9665XXXXXXXX
VITE_WHATSAPP_NUMBER="966500000000"

# Google Maps API key for map functionality
# Get your API key from https://developers.google.com/maps/documentation/javascript/get-api-key
VITE_GOOGLE_MAPS_API_KEY="YOUR_GOOGLE_MAPS_API_KEY"
